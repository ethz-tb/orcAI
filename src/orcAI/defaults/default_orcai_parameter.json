{
    "name": "orcai-v1",
    "architecture": "ResNetLSTM",
    "model": {
        "epochs": 30,
        "batch_size": 32,
        "filters": [
            30,
            40,
            50,
            60
        ],
        "kernel_size": 5,
        "dropout_rate": 0.4,
        "lstm_units": 64,
        "n_batch_train": 3200,
        "n_batch_val": 320,
        "n_batch_test": 320,
        "shuffle": true,
        "EarlyStopping_patience": 10,
        "ReduceLROnPlateau_patience": 3,
        "ReduceLROnPlateau_factor": 0.5,
        "ReduceLROnPlateau_min_learning_rate": 1e-6,
        "call_weights": null,
        "monitor": "val_MBA"
    },
    "spectrogram": {
        "sampling_rate": 48000,
        "nfft": 512,
        "n_overlap": 256,
        "freq_range": [
            0,
            16000
        ],
        "quantiles": [
            0.01,
            0.999
        ],
        "duration": 4
    },
    "calls": [
        "BR",
        "BUZZ",
        "HERDING",
        "PHS",
        "SS",
        "TAILSLAP",
        "WHISTLE"
    ],
    "snippets": {
        "segment_duration": 200,
        "snippets_per_sec": 3,
        "snippet_duration": 4,
        "fraction_removal": 0.99,
        "train": 0.8,
        "val": 0.1,
        "test": 0.1
    }
}
